<!-- Start features section -->
<section id="mu-features">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="mu-features-area">
                    <!-- Start Title -->
                    <div class="mu-title">
                        <h2>Categories</h2>
                    </div>
                    <!-- End Title -->
                    <!-- Start features content -->
                    <div class="mu-features-content">
                        <div class="row">
                            @foreach (var item in ViewBag.Categories)
                            {
                                <div class="col-lg-4 col-md-4  col-sm-6">
                                    <div class="mu-single-feature">
                                        <span class="fa fa-book"></span>
                                        <h4>
                                            @item.name
                                        </h4>
                                        <p>
                                            Lorem ipsum dolor sit amet, consectetur adipisicing elit. Accusamus non dolorem excepturi libero
                                            itaque sint labore similique maxime natus eum.
                                        </p>
                                        @if (Session["userId"] != null)
                                        {
                                            @Html.ActionLink("Suscribe", "Subscribe", new { id = item.id, userId = @Session["userId"] })
                                        }
                                    </div>
                                </div>

                            }
                        </div>
                    </div>
                    <!-- End features content -->
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End features section -->
<!-- Start our teacher -->
<section id="mu-our-teacher">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="mu-our-teacher-area">
                    <!-- begain title -->
                    <div class="mu-title">
                        <h2>Feature Events</h2>
                    </div>
                    <!-- end title -->
                    <!-- begain our teacher content -->
                    <div class="mu-our-teacher-content">
                        <div class="row">
                            @foreach (var item in ViewBag.Activity)
                            {
                                <div class="col-lg-3 col-md-3  col-sm-6">
                                    <div class="mu-our-teacher-single">
                                        <figure class="mu-our-teacher-img">
                                            <img src='~/assets/images/@item.image' alt="teacher img">
                                            <div class="mu-our-teacher-social">
                                                @if (Session["userId"] != null)
                                                {
                                                    @Html.ActionLink("Join", "GotoEvent", new { id = item.id, userId = @Session["userId"] })
                                                }
                                            </div>
                                        </figure>
                                        <div class="mu-ourteacher-single-content">
                                            <a href="/Home/ViewEvent/@item.id"><h4>@item.name</h4></a>
                                            <span>@item.topic</span>
                                            <p>
                                                @item.description
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                    <!-- End our teacher content -->
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End our teacher -->
<!-- Start our teacher -->
<section id="mu-our-teacher">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="mu-our-teacher-area">
                    <!-- begain title -->
                    <div class="mu-title">
                        <h2>Event Winners</h2>
                    </div>
                    <!-- end title -->
                    <!-- begain our teacher content -->
                    <div class="mu-our-teacher-content">
                        <div class="row">
